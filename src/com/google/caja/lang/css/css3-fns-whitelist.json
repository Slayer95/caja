{
  "allowed": [
    "inset",
    "cyrcle",
    "ellipse",
    "polygon",
    "rect",
    "rgb",
    "rgba",
    "hsl",
    "hsla",
    "blur",
    "brightness",
    "contrast",
    "drop-shadow",
    "grayscale",
    "hue-rotate",
    "invert",
    "opacity",
    "saturate",
    "sepia",
    "url" ],

  "denied": [
    { "key": "attr",
      "reason": "unknown semantics" },

    { "key": "counter",
      "reason": "affects global state" },

    { "key": "expression",
      "reason": "executes arbitrary javascript" } ]
}
