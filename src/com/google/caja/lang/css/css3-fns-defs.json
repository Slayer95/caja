{
  "description": [
    "Defines functions allowed in CSS.",
    "See http://www.w3.org/TR/css3-values/#functional"
    ],

  "types": [
    { "key": "counter",
      "signature": "<ident> <list-style-type>?" },

    { "key": "calc",
      "signature": "<length-expression>" },

    { "key": "attr",
      "signature": "<ident> ((string|value|url|integer|number|length|angle|time|frequency|em|ex|px|gd|rem|vw|vh|vm|mm|cm|in|pt|pc|deg|grad|rad|ms|s|Hz|kHz|percentage) <value>?)?" },

    { "key": "inset",
      "signature": "[ <length> | <percentage> ]{1, 4} [ round <border-radius> ]?" },

    { "key": "circle",
      "signature": "[ <length> | <percentage> | closest-side | farthest-side ]? [ at <position> ]?" },

    { "key": "ellipse",
      "signature": "[ [ <length> | <percentage> | closest-side | farthest-side ] [ <length> | <percentage> | closest-side | farthest-side ] ]? [ at <position> ]?" },

    { "key": "rect",
      "signature": "<top> <right> <bottom> <left>" },

    { "key": "polygon",
      "signature": "<fill-rule>? [ <length> | <percentage> ] [ <length> | <percentage> ] [ [ <length> | <percentage> ] [ <length> | <percentage> ] ]*" },

    { "key": "rgb",
      "signature": "<red> <green> <blue>" },

    { "key": "rgba",
      "signature": "<red> <green> <blue> <alphavalue>" }

    { "key": "hsl",
      "signature": "<angle> <percentage> <percentage>" },

    { "key": "hsla",
      "signature": "<angle> <percentage> <percentage> <alphavalue>" }

    { "key": "url",
      "signature": "<uri>|<string>" }
  ]
}
